	"license": "MIT",
	"bugs": {
		"url": "https://github.com/likexoo/fake/issues"
	},
	"homepage": "https://github.com/likexoo/fake#readme",
	"devDependencies": {
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-typescript": "^7.12.7",
		"@types/jest": "^26.0.19",
		"@typescript-eslint/eslint-plugin": "^4.11.0",
		"eslint": "^7.16.0",
		"jest": "^26.6.3",
		"typescript": "^4.1.3"
	}
